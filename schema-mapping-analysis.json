			"folderStructure": "<moduleName>/<featureName>/_schema/<tableName>.ts",
			"fileNaming": "camelCase for file names (e.g., semesterModules.ts, studentPrograms.ts)",
			"enumOwnership": "Enums belong to the PRIMARY feature that uses them and are imported by other features via @/core/database",
			"crossModuleImports": "All tables and relations should be imported from @/core/database",
			"indexAggregation": "Each module keeps <moduleName>/_database/index.ts to aggregate all feature schemas",
			"relationsFile": "Each feature has its own relations.ts in _schema folder"
		}
